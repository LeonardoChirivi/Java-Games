package snake;

public class SnakeTile {
	private int x;
	private int y;
	private SnakeTile next;
	
	public SnakeTile( int x, int y, SnakeTile next ){
		this.x = x;
		this.y = y;
		this.next = next;
	}
	
	public int getX(){
		return this.x;
	}
	
	public int getY(){
		return this.y;
	}
	
	public SnakeTile getNext(){
		return this.next;
	}
	
	public void setNext( SnakeTile next ){
		this.next = next;
	}
}
