package snake;

public class Tile {
	
	protected int x;
	protected int y;
	private final int WIDTH = 10;
	private final int HEIGHT = 10;
	
	public Tile() {}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getWIDTH() {
		return WIDTH;
	}

	public int getHEIGHT() {
		return HEIGHT;
	}
}
