package snake;

public class SnakeTile {
	private final int WIDTH = 10;
	private final int HEIGHT = 10;
	
	private int x;
	private int y;
	private SnakeTile next = null;
	
	public SnakeTile( int x, int y ){
		this.x = x;
		this.y = y;
	}
	
	public int getX(){
		return this.x;
	}
	
	public int getY(){
		return this.y;
	}
	
	public SnakeTile getNext(){
		return this.next;
	}
	
	public void setNext( SnakeTile next ){
		this.next = next;
	}
}
